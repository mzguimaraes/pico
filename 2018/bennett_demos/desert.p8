pico-8 cartridge // http://www.pico-8.com
version 15
__lua__
--desert walk--
--bennett foddy, 2018--

--object to track player
player={x=128,y=64,f=0,alive=true}
--track npc
stranger={x=768,y=56,on=true}
steps=0
lastspokeat=0
--array for particles
dust={}
--list of dialog text
dialog={
"",
"i'm so tired","",
"i can barely see","",
"i need some water","",
"feeling very cold","","",
"not sure i can go on",""
}
dialogindex=1 --current dialog
dialog2={
 "howdy, partner",
 "did you get lost, son?",
 "you look terrible",
 "i can help you... if you want"
}
dialogindex2=1
strangertimer=0

--create dust array
for i=1,100 do
 --each dust particle is just
 --an x,y position and x,y speed
 add(dust,{x=rnd(128), y=rnd(128),
  vx=-(rnd(3)+3),vy=0})
end

--object for holding camera pos
cam={x=96,y=128}

--this function runs once
function _init()
 music(0) --start the music
end

--this function runs 30 times/sec
function _update()

 if player.alive==false then
  return --game over
 end
 
 --handle npc timer
 if strangertimer>0 then
  strangertimer-=1
  if strangertimer<=0 then
   dialogindex2+=1 --ready for next line
   stranger.on=false
  else
   return --ignore rest of function
  end
 end
  
 --process player input
 if btn(➡️) then
  player.x += 1
  player.f += 1 --animate while walking
  steps+=1
 elseif btn(⬅️) then 
  player.x -= 1
  player.f += 1
  steps+=1
 end
 
 --wrap around if player reaches
 --edge of map
 while player.x > 1024 do
  player.x -=1024
  cam.x -=1024
 end
 while player.x < 0 do
  player.x +=1024 
  cam.x += 1024
 end
 
 --wrap the animation timer
 if player.f>30 then 
  player.f=0 
 end
 
 --footsteps
 if player.f%15==1 then
  sfx(5) 
  player.f+=1
 end

	--run the dialog
	local strangerdist=abs(player.x-stranger.x)
	--npc speaks if enabled and close
	if stranger.on and
	    strangerdist < 8 then
	 				strangertimer=120
	 				sfx(3)
	end
	--reset npc if we're far away
	if stranger.on==false and
	    strangerdist > 300 then
	     stranger.on=true --reset
	end
	--player speaks every 200 steps
	if strangertimer<=0 and
	    steps-lastspokeat > 200 then
	     lastspokeat=steps
	     dialogindex+=1
	     --die if out of dialog
	     if dialogindex>#dialog then
	      if player.alive then
	       player.alive=false--die
	       sfx(5)
	      end
	     end
	end

end

--pico8 has no inbuilt lerp
function lerp(a,b,t)
 return a*(1-t) + b*t 
end


function _draw()
 cls() --clear screen
 
 --lerp camera to player
 cam.x=lerp(cam.x,player.x-32,0.03)
 cam.y=lerp(cam.y,player.y-60,0.03)
 
 --set draw offset with cam pos
 camera(cam.x,cam.y)
 
 --clip the view to a thin rectangle
 clip(0,32,128,64)
 
 --swap colors to give a feeling
 --of time passing
	local hour=(flr(t()*0.25))%4
	if (hour==0) then
	 pal(15,15)
	 pal(7,7)
	elseif (hour==1) then
	 pal(15,10)
	 pal(7,9)	 
	elseif (hour==2) then
	 pal(15,6)
	 pal(7,13)
	elseif (hour==3) then
	 pal(15,14)
	 pal(7,6)	 
	end 
 
 --draw the map 3x so it wraps
 map(0,0,-1024,0,128,31)
 map(0,0,0,0,128,31)
 map(0,0,1024,0,128,31)
 
 --change the transparent color
 --for drawing sprites
 palt(0,false)--set black opaque
 palt(11,true)--set green transparent
 
	--draw and animate player
	if player.alive then
 	if player.f < 15 then
 		spr(4,player.x,player.y,2,1)
 	else
 	 spr(6,player.x,player.y,2,1)
 	end
	else 
	 spr(20,player.x,player.y,2,1)
	end
	
	--draw npc
	if stranger.on then
	 spr(52,stranger.x,stranger.y,2,1)
	end
	 
	--move and draw dust 
 for i,d in pairs(dust) do
  d.x += d.vx
  d.y += d.vy
  d.vy = sin(time()+i*0.1)*1
  --wrap at screen edge
  while d.x-cam.x > 128 do d.x-=128 end
  while d.x-cam.x < 0 do d.x += 128 end
  while d.y-cam.y > 128 do d.y-=128 end
  while d.y-cam.y <0 do d.y += 128 end
  --draw pixel at dust pos
  pset(d.x,d.y,15)
 end

	--reset camera so we can draw ui
	camera()
	--unclip the view
	clip()
	--reset the palette
 pal() 
 
	if strangertimer>0 then
	 --print stranger dialog
	 color(8)
	 local text=dialog2[dialogindex2]
	 print(text,126-#text*4,98) --right-aligned
 else
  color(6)
  --print player dialog
  if player.alive then
   print(dialog[dialogindex],2,98)
  end
 end
end
