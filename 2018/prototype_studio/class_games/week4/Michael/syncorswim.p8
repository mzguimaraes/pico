score=0, swum=false}

bdcheck0= 0
bdcheck1= 0

ymod = -10

arrows={}

--keys{}

function _init()

end

function _update60()
--makes the men sink
	p0.y += .05
	p1.y += .05
	
	p1.t-=1
 p0.t-=1
 
 if btn(❎,0) == false then
 	bdcheck0 += 1
 end
 if btn(❎,1) == false then
 	bdcheck1 += 1
 end
	
	for player=0,1 do
		if btn(❎,player) then
   drown(player)
 	end
	end	
end

function _draw()
cls()


spr(p1.s,p1.x,p1.y,2,2)
spr(p0.s,p0.x,p0.y,2,2)

map(0,0,0,0,10,10)



end
-->8
function drown(p) 

 if p==1 then
 	if bdcheck1 > 1 then
 		p1.y -= 1
 		bdcheck1 = 0
 	end
 elseif p==0 then
 	if bdcheck0 > 1 then
 		p0.y -= 1
 		bdcheck0 = 0
 		end
 	end
end 
-->8
function rightarrow(p)
	ra={}
	
	if p == 1 then
		ra.x = 100,
	 ra.y = 100,
	 ra.dx = 0,
	 ra.dy = ymod,
	 add{ra,arrows}
	end
	 
	 

end
