 		if(p1.i >= 120) then
 			 if(dogf > 15) then
 						p1.s = 96
 					if(dogf >= 45) then
 						dogf = 0
 					end
 				else
 						p1.s = 112
 				end
 				
 			p1.h = 8
 		else	
