function _update60()
	
	dogf+=1
	dogf2+=1
	
		if(p1.grounded == false) then
  	p1.vy+=gravity
 	end
  
  --handle movement buttons
  if btn(âž¡ï¸) then
   p1.vx=1
   p1.f=false 
  elseif btn(â¬…ï¸) then
   p1.vx=-1
   p1.f=true --flip if going left
  else p1.vx*=0.8 --slow to a stop
  end
  
  --jump if grounded
  if btnp(ðŸ…¾ï¸) and p1.grounded then
   p1.vy=-6
   sfx(4)
  elseif btnp(ðŸ…¾ï¸) then
   p1.vy-=2
   sfx(5)
  else p1.vy*=0.9
 end 
 
 if(p1.x > 125) then
