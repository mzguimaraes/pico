function _spawnfoes(x)
	
	for i=0,x do
		add(enemies,{x=i*100+40,y=30,w=15,h=14,vx=0.15,
	vy=0,rx=0,ry=0,s=78,f=true,t=0,
	grounded = false, i=0, health=3,
	alive = true, hurt = false,active = false,
	f0 = 192, f1 = 78,f2= 110,
	spd = 0.15})
	end
end
