--draws the clouds (moves it too)
function _drawclouds()
	for c in all(clouds) do
	c.x -= 0.16
	
	if(c.c < 1) then
		spr(168,c.x, c.y,1,1)
	elseif(c.c <2) then
		spr(184,c.x, c.y,1,1)
	elseif(c.c < 3) then
		spr(169,c.x, c.y,1,1)
	else
		spr(185,c.x, c.y,1,1)
	end
	
	if(c.x < -72) then
		c.x = 160 + rnd(160)
		c.y = 6+rnd(30)
	end
end
end

