__lua__
testf = 0
mapx = 0
mapy = 0
cam = {}
clouds = {}
shots = {}
enemies = {}
numen = 6
cam = {x = 0,y=0}
state = 0


gravity = 0.3
activeshot = 0

--player variables held here
p1={x=19,y=56,w=9,h=20,vx=0,health = 5,
vy=0,rx=0,ry=0,s=66,f=false,t=0,
grounded=false,i=0, attack = false,
hurt = false}

function _init()
 music()
 _spawncloud(16)
 _spawnfoes (numen)
end

function _update60()
	
	cam.x = mapx
	cam.y = mapy
	
	if(p1.grounded == true) then
		if(btnp(5,0)) then
			p1.attack = true
			p1.t = -20
		end
	end

	--"gravity" increases while not grounded
	if(p1.grounded == false) then
  	p1.vy+=gravity
 	end
 if(p1.hurt == false) then
 -- if(attack == false) then
  --handle movement buttons
  if btn(➡️) then
   p1.vx=0.85
   p1.f=false 
  elseif btn(⬅️) then
   p1.vx=-0.85
   p1.f=true --flip if going left
  else
  --instantly stops movement if
  --player is not holding a key
 		
  		p1.vx = 0
  end
 
 --while hurt...
 else
 	p1.s = 194
 	p1.t+= 1
 	if(p1.t > 30) then
 		p1.hurt = false
 	end
 
 end
 -- end
  --jump/gravity related controls
  if btnp(🅾️) and p1.grounded then
   p1.vy=-6
   sfx(4)
   

  else p1.vy*=0.9
 end 
 

 
 if(p1.x > 125) then
 	p1.x = 0
 	for f in all(enemies) do
 		f.x -= 16*8
 	end
 	mapx+=16
 end
 
 if(p1.x < -5) then
 	p1.x = 120
 	for f in all(enemies) do
 		f.x += 16*8
 	end
 	mapx-=16
 end
 
 if(p1.y > 65) then
 	p1.y = 2
 	for f in all(enemies) do
 		f.y -=8
 	end
 	mapy += 8
 end
 
 if(p1.y < 1) then
 	p1.y = 64
 	
 	for f in all(enemies) do
 		f.y +=8
 	end
 	
 	mapy -= 8
 end
	
	if(p1.attack == true) then
		
			move(p1,0,p1.vy,nil,landonwall)
	else
 	move(p1,p1.vx,p1.vy,nil,landonwall)
	end
end


function _draw()
	cls()
	
	if(state == 1) then
		print("victory!!",50,50,11)
		print("cntrl-r to restart",30,62,1)
	end
	
	if(state == 2) then
				print("game over",46,50,8)
		print("cntrl-r to restart",30,62,1)
	end
	
	--main gameplay
	if(state == 0) then 
	_drawsky()
	
	--camera(cam.x-64,cam.y-40)
 palt(10,true) 
 palt(0,false) 
 map(mapx+cam.x,mapy+cam.y,0,16,256,256)
 
 
 _drawui()
 
 --increments player anim timer
	if(p1.vx > 0) or (p1.vx<0)
	or(p1.t < 0) then
		p1.t += 1	
	else
		p1.t = 0
	end
	
	if(p1.attack == true)then
		p1.w = 24
		if(p1.t >= 0) then
			p1.attack = false
		end
	else
		p1.w = 9
	end
	
	--player walking animation here
	if(p1.t <= 0) then
		if(p1.attack == true)then
			p1.s = 74
		else
			p1.s = 64
		end
	elseif(p1.t < 10) then
		p1.s = 66
	elseif(p1.t < 20) then
		p1.s = 68
	elseif(p1.t < 30) then
		p1.s = 70
	elseif(p1.t < 40) then
 	p1.s = 72
	end
	
	--resets anim here
	if(p1.t >= 39) then
		p1.t = 1
	end
	
	--player is drawn here
	if(p1.attack == true) and
	(p1.f == true) then
		spr(p1.s,p1.x-8,p1.y+(16-p1.h),
		flr(p1.w/8+0.5),flr(p1.h/8+0.5),p1.f)
	else
		spr(p1.s,p1.x,p1.y+(16-p1.h),
		flr(p1.w/8+0.5),flr(p1.h/8+0.5),p1.f)
	end
	
	--moves and draws enemy
	for f in all(enemies) do
		
		if(f.alive == true) then
		--applies gravity to them here
		if(f.grounded == false) then
			f.vy += gravity
		else
			f.vy = 0
		end
		
		--turns to face player
		if(p1.x < f.x-(mapx*8)) and (f.vx > 0) then
			f.vx *= -1
			f.f = false
		elseif(p1.x > f.x-(mapx*8)) and (f.vx < 0) then
			f.vx *= -1
			f.f = true
		end
		
		--if touching player
		if(_animcol(p1,f)) then
			if(p1.attack == true) and
			(f.hurt == false) then
				f.hurt = true
				f.s = f.f0
				f.health -=1
				f.t = 0
			end
			
			if(p1.attack == false) and
			(p1.hurt == false) then
				p1.vx = f.vx * 10
				p1.vy = -5
				p1.hurt = true
				p1.t = -15
				p1.health -= 1
				_shakecam(1)
				f.s = f.f0
				f.t = 0
				f.vy = -5
			end
		
		end
		
			f.t += 1
		--if hurt, picks hurt sprite
		if(f.hurt == false) then
			--picks sprite
 		if(f.t < 15) then
				f.s = f.f1
			elseif(f.t < 30) then
				f.s = f.f2
			end
		else
			f.s = f.f0
		end
		--animation state reset
		if(f.t >= 29) then
			f.t = 0
			f.hurt = false
			
			--bullet fired here
 		if(rnd(1) < 0.3) then
			
			end
		end
		
		if(f.hurt == false) then
			move(f,f.vx,f.vy,nil,landonwall)
			reversernd(f)
			spr(f.s,f.x,f.y,2,2,f.f)
		else
			move(f,-f.vx,f.vy,nil,landonwall)
			spr(f.s,f.x,f.y,2,2,f.f)
		end
		
		
	
		--enemy dies with no health
		if(f.health <= 0) then
			f.alive = false
			numen -= 1
		end
		
		--all enemies dead - victory
		if(numen <= 0) then
			state = 1
		end
		
		--p1 health gone - defeat
		if(p1.health <= 0) then
			state = 2
		end
	end
	end
 end --end state 1
end

