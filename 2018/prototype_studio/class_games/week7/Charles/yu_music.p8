function _drawui()
	sspr(56,64,8,8,0,88,16*8,16*8)
	sspr(56,64,8,8,0,0,16*8,16)
	
	--draws the player's health
	spr(240,2,4,1,1)
	line(11,6,11+(p1.health*7),6,8)
	line(11,7,11+(p1.health*7),7,8)
	line(11,8,11+(p1.health*7),8,8)
end


