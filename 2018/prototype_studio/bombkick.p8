--all object spawning on this tab

--add an inactive bomb and reset it
function addbomb(x,y)
 for b in all(bombs) do
  --look for a dead bomb
  --and revive it at x,y
  if b.t <=0 then
   b.x=x
   b.y=y
   b.vx=0
   b.vy=0
   b.rx=0
   b.ry=0
   b.grounded=false
   b.t=flr(rnd(timebetweenbombs)
          +timebetweenbombs)
   timebetweenbombs-=1
   return
  end
 end
end

--add an inactive explosion and reset it
function addexplosion(x,y)
 for e in all(explosions) do
  --look for a dead explosion
  --and revive it at x,y
  if e.t <=0 then
   e.x=x
   e.y=y
   e.t=4
   for i=1,10 do
   	emitspark(x,y,4)
   end
   sfx(2)
   return
  end
 end
end

--add an inactive spark
--and reset it
function emitspark(x,y,speed)
 for s in all(sparks) do
  if s.life < 1 then
   s.life=12 --bring it to life
   s.x=x
   s.y=y
   s.vx=rnd(2*speed)-speed --random velocity
   s.vy=rnd(2*speed)-2*speed
   return
  end
 end
end
