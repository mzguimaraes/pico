--â—†bomb kickâ—†--
--bennett foddy, 2017-2018
--this one is the most complex
--but still <300 lines of code

--set up a player object
p1={x=16,y=16,w=8,h=8,vx=0,
vy=0,rx=0,ry=0,s=1,f=false,t=0,
grounded=false,kick=0,alive=true}
--make some arrays
bombs={}
spawns={}
explosions={}
sparks={}
gravity = 0.3
--an array of spark colors
--to get a nice sparky animation
sparkcols={1,1,8,8,9,9,10,10,7,7,7}
map_w=16
map_h=16
timeuntilbomb=0
timebetweenbombs=200
score=0
 
function _init()

	--pool a bunch of inactive
	--objects to track bombs,
	--explosions and sparks
 for i=0,100 do  
  add(bombs,{
   x=0,y=0,vx=0,vy=0,rx=0,ry=0,
   grounded=false,t=0
  })
  add(explosions,{
   x=0,y=0,t=0
  })
  add(sparks,{x=0,y=0,life=0,
   vx=0,vy=0}
  )
 end
 
 --look in the map for eggs
 --if an egg is found, that's a
 --bomb spawn point
	for col=0,map_w do
	 for row=0,map_h do
	  if mget(col,row)==16 then
	   add(spawns,{x=col*8,
	   y=row*8})
	  end
	 end
	end
end

function _update60()
	if p1.alive then
  p1.vy+=gravity
  
  --handle movement buttons
  if btn(âž¡ï¸) then
   p1.vx=1
   p1.f=false 
  elseif btn(â¬…ï¸) then
   p1.vx=-1
   p1.f=true --flip if going left
  else p1.vx*=0.8 --slow to a stop
  end
  
  --jump if grounded
  if btnp(ðŸ…¾ï¸) and p1.grounded then
   p1.vy=-6
   sfx(4)
  elseif btnp(ðŸ…¾ï¸) then
   p1.vy-=2
   sfx(5)
  else p1.vy*=0.9
  end 
  
  --kick
  if btnp(âŽ) then 
   p1.kick=2 --reset timer
   
   --get array of nearby bombs
   local bs=overlapbombs(p1.x,p1.y,
            p1.x+8,p1.y+8)
		 
		 --kick those bombs
		 for b in all(bs) do
		  --which side of p1 is bomb on
		  local d=sgn(b.x-p1.x)
		  b.vx=d*5+rnd(2) --push bomb
		  b.vy=-3-rnd(2)
		 end
		 if #bs>=1 then 
		  sfx(1)
		  score+=#bs
		 else sfx(0) end --missed kick
  end
  
  --move and collide the player
  --according to its velocity
  move(p1,p1.vx,p1.vy,nil,landonwall)

 end
   
 --update bombs
 timeuntilbomb-=1
 if timeuntilbomb <=0 then
   timeuntilbomb=rnd(200)+10
   --find a spawn point
   local i=flr(rnd(#spawns))
   local s=spawns[i+1]
   addbomb(s.x,s.y)
 end
 for b in all(bombs) do
  if b.t > 0 then
   b.vy+=gravity
   if b.grounded then b.vx*=0.9 end
   move(b,b.vx,b.vy,bouncex,landonwall)
  end
 end
 
 --update explosions
 for e in all(explosions) do
  if e.t==3 then --new explosion
   local bs = overlapbombs(e.x-8,e.y-8,e.x+16,e.y+16)
   for b in all(bs) do
    b.t=1 --chain explode
    sfx(6)
   end
   
   if p1exploded(e) then
    p1.alive=false
    sfx(7)
   end
  end
 end
end

function _draw()
 cls()
 --handle player animation
 p1.t+=1 --timer increases
 if p1.t>11 then p1.t=0 end
 
 --set sprite for player
 if p1.alive==false then
  p1.s=48 --blood frame
 elseif p1.kick>0 then
  p1.kick-=0.25
  p1.s=17+flr(p1.kick) --kick frames
 elseif p1.grounded==false then
  if p1.vy < 0 then
   p1.s=2 --jumping up
  else
   p1.s=19 --falling
  end
 elseif flr(abs(p1.vx)+abs(p1.vy))>0 then
  p1.s=2+flr(p1.t/4) --walk animation
 else
  p1.s=1 --idle frame
 end
  
 
 --draw player
 spr(p1.s,p1.x,p1.y,1,1,p1.f)
 
 --draw bombs
 for b in all(bombs) do
  b.t-=1 --update timer
  if b.t>0 then
 	 --set frame from timer
  	local f=32+flr(b.t/timebetweenbombs*2)
  	spr(f,b.x,b.y)
  	if (f>32) then 
  	 emitspark(b.x+7,b.y+2,1)
  	end
  elseif b.t==0 then
   addexplosion(b.x,b.y)
  end
 end 
 
 --draw the tilemap
 map(0,0,0,0,map_w,map_h,1)

 --draw sparks
 for s in all(sparks) do
  if s.life>0 then  
   --first update positions
   s.vy+=gravity
   s.x+=s.vx
   s.y+=s.vy
   --draw a pixel
   pset(s.x,s.y,sparkcols[s.life])
   s.life-=1 --update life
  end
 end
 
 --draw explosions
 for e in all(explosions) do
  if e.t >0 then
   e.t -=1 --update the timer
   spr(23+e.t*3,e.x-8,e.y-8,3,3)
  end
 end
 
 --draw score
 print("bombs kicked: "..score,0,120,7)
   
end
