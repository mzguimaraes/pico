pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
--a bad game
--by marcus zaeyen

function _init()
	cam.following=player
end

function _update60()
	cam:update()
	player:update()
	
	textchanger:update()
	
	if not gameover and player.x>=raceend then
		gameover=true
		racetime=time()
	end
	

end


text=""
function _draw()
	local clrcol=12
	if player.x>9*(raceend/10) then
		clrcol=2
	elseif player.x>raceend/2 then
		clrcol=13
	end
	
	local suncol=10
	if player.x>3*raceend/4 then
		suncol=9
	end
	cls(clrcol)
	camera(cam.x,cam.y)
	
	circfill(
		cam.x+10+96*((raceend-player.x)/raceend),
		24,10,suncol)
	rectfill(
		cam.x,groundy,
		cam.x+128,128,
		4
	)
	player:draw()
	
	if not gameover then
		print("race!\n"..
			((raceend-player.x)/75).."m left!\n"..
			time(),
			cam.x,cam.y,7)
	else
		print("you finished!\n"..
		"final time: "..racetime,
		cam.x,cam.y,7)
	end
	
	print(text,cam.x,110,flr(rnd(16)))
end
-->8
--object definitions

cam={x=0,y=0,stress=0,
	following=nil, followspeed=.15,
	update=function(self)
		
		if self.following~=nil then
			
			self.x=flr((lerp(self.x,
				self.following.x-56,
				self.followspeed)))
			--self.y=flr((lerp(self.y,
			--	self.following.y-64,
			--	self.followspeed)))
		end
		
		if self.stress>0 then
			local r = rnd()
			self.x=cos(r)*self.stress
			self.y=sin(r)*self.stress
			
			self.stress=lerp(self.stress,0,.15)
			if self.stress<.05 then
				self.stress=0
			end
		end
	end
}

player = {
	x=0,y=0,
	dx=0,dy=0,
	ddx=0,ddy=0,
	grounded=false,
	speed=.05,
	jumpforce=5,
	
	update=function(self)
		--grounded check
		if self.y>=groundy then
			self.grounded=true
			--cam.stress=5
		else
			self.grounded=false
		end
		
		--input
		if not self.grounded then
			if btn(⬅️) then
				self.ddx-=self.speed
			elseif btn(➡️) then
				self.ddx+=self.speed
			end
		else
			self.dy=0
			self.ddy=0
			self.dx=lerp(self.dx,0,.25)
			if btnp(❎) or btnp(⬆️) then
				self.ddy-=self.jumpforce
			end
		end
	
		--gravity
		if not self.grounded then
			self.dy+=grav
		else
			self.dy=min(self.dy,0)
		end
		
		--accelerate
		self.dx+=self.ddx
		self.dy+=self.ddy
		
		--move
		self.x+=self.dx
		self.y+=self.dy
		
		self.ddx=0
		self.ddy=0
		
	end,
	
	draw=function(self)
		spr(1,self.x,self.y)
	end
}

textchanger={
	messages={
		"long way to go",
		"you don't have anything better\nto do?",
		"you could be playing a better\ngame",
		"marcus will forgive you\nif you turn this off",
		"my mother used to be proud of me",
		"narrative games don't have to\nbe good",
		"my parents paid $40k/year\nfor me to learn to make this",
		"why are you still here?",
		"guess you might as well finish",
		"congrats i guess"
	},
	update=function(self)
		for i=1,#self.messages do
			if player.x>i*(raceend/10) then
				text=self.messages[i]			
			end
		end
	end
}
