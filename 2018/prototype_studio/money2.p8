function _update60()
	bird:update()
	for c in all(coins) do
		c:update()
	end
	for w in all(walls) do
		w:update()
	end
	
	coinmgr:update()
	wallmgr:update()
	physics:update()
end

function _draw()
	cls(12)
	ground:draw()
	
	for c in all(coins) do
		c:draw()
	end
	for w in all(walls) do
		w:draw()
	end
	
	print("flappy dad",1,113,5)
	print("flappy dad",0,112,7)

	print("flaps: "..bird.flaps,
		1,121,5)
	
	print("flaps: "..bird.flaps,
		0,120,7)
	
	bird:draw()
end
